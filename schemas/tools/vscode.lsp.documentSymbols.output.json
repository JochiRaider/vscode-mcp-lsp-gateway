{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schemas/tools/vscode.lsp.documentSymbols.output.json",
  "title": "vscode.lsp.documentSymbols â€” output schema",
  "description": "StructuredContent for vscode.lsp.documentSymbols.",
  "type": "object",
  "additionalProperties": false,
  "required": ["symbols"],
  "properties": {
    "symbols": {
      "type": "array",
      "items": { "$ref": "#/$defs/documentSymbol" }
    },
    "summary": {
      "type": "string",
      "minLength": 1
    }
  },
  "$defs": {
    "position": {
      "type": "object",
      "additionalProperties": false,
      "required": ["line", "character"],
      "properties": {
        "line": { "type": "integer", "minimum": 0 },
        "character": { "type": "integer", "minimum": 0 }
      }
    },
    "range": {
      "type": "object",
      "additionalProperties": false,
      "required": ["start", "end"],
      "properties": {
        "start": { "$ref": "#/$defs/position" },
        "end": { "$ref": "#/$defs/position" }
      }
    },
    "documentSymbol": {
      "type": "object",
      "additionalProperties": false,
      "required": ["id", "name", "kind", "range", "selectionRange"],
      "properties": {
        "id": { "type": "string", "minLength": 1, "pattern": "^sha256:" },
        "name": { "type": "string", "minLength": 1 },
        "kind": { "type": "integer", "minimum": 0 },
        "range": { "$ref": "#/$defs/range" },
        "selectionRange": { "$ref": "#/$defs/range" },
        "containerName": { "type": "string", "minLength": 1 }
      }
    }
  }
}
